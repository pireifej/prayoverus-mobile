{
  "expo": {
    "name": "PrayOverUs",
    "slug": "pray-over-us",
    "version": "1.0.5",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "scheme": "prayoverus",
    "splash": {
      "image": "./assets/icon.png",
      "resizeMode": "contain",
      "backgroundColor": "#6366f1"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.pireifej.prayoverus",
      "associatedDomains": ["applinks:prayoverus.com"],
      "infoPlist": {
        "ITSAppUsesNonExemptEncryption": false
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/icon.png",
        "backgroundColor": "#6366f1"
      },
      "package": "com.pireifej.prayoverus",
      "versionCode": 5,
      "permissions": [
        "CAMERA",
        "READ_EXTERNAL_STORAGE",
        "WRITE_EXTERNAL_STORAGE",
        "NOTIFICATIONS"
      ],
      "intentFilters": [
        {
          "action": "VIEW",
          "autoVerify": true,
          "data": [
            {
              "scheme": "https",
              "host": "prayoverus.com",
              "pathPrefix": "/reset-password"
            }
          ],
          "category": ["BROWSABLE", "DEFAULT"]
        }
      ],
      "useNextNotificationsApi": true
    },
    "notification": {
      "icon": "./assets/icon.png",
      "color": "#6366f1",
      "androidMode": "default",
      "androidCollapsedTitle": "PrayOverUs"
    },
    "plugins": [
      [
        "expo-notifications",
        {
          "icon": "./assets/icon.png",
          "color": "#6366f1",
          "sounds": []
        }
      ]
    ],
    "extra": {
      "eas": {
        "projectId": "91bdb01b-1012-49e1-abb4-fc8fae904ec8"
      }
    },
    "owner": "pireifej",
    "runtimeVersion": {
      "policy": "appVersion"
    },
    "updates": {
      "url": "https://u.expo.dev/91bdb01b-1012-49e1-abb4-fc8fae904ec8"
    }
  }
}
